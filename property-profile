            { category: 'Investment', score: 90, status: 'high' },
            { category: 'Rating', score: 62, status: 'moderate' },
            { category: 'Market Capture', score: 89, status: 'high' }
        ];
        
        const zipData = [
            { zip: '68803', sales: 54.2, x: 50, y: 45, color: '#1E3A5F' },
            { zip: '68801', sales: 18.5, x: 28, y: 58, color: '#2E5A87' },
            { zip: '68802', sales: 12.3, x: 72, y: 32, color: '#4A90D9' },
            { zip: '68847', sales: 6.8, x: 22, y: 78, color: '#6B9BD2' },
