            { zip: '68862', sales: 2.7, x: 82, y: 22, color: '#A8C5E5' },
            { zip: 'Other', sales: 1.3, x: 15, y: 32, color: '#C5DAEF' }
        ];
        
        const ageData = [
            { range: '18-24', shoppers: 16, residents: 11 },
            { range: '25-34', shoppers: 19, residents: 32 },
            { range: '35-44', shoppers: 22, residents: 18 },
            { range: '45-54', shoppers: 21, residents: 15 },
            { range: '55-64', shoppers: 14, residents: 12 },
            { range: '65+', shoppers: 8, residents: 12 }
        ];
        
        const incomeData = [
            { range: '<$50K', shoppers: 15, residents: 28 },
            { range: '$50-75K', shoppers: 18, residents: 25 },
            { range: '$75-100K', shoppers: 22, residents: 20 },
            { range: '$100-150K', shoppers: 28, residents: 18 },
            { range: '$150K+', shoppers: 17, residents: 9 }
        ];
        
        const monthlyTrend = [
            { month: 'Dec', sales: 298 },
            { month: 'Jan', sales: 275 },
            { month: 'Feb', sales: 268 },
            { month: 'Mar', sales: 285 },
            { month: 'Apr', sales: 292 },
            { month: 'May', sales: 305 },
            { month: 'Jun', sales: 312 },
            { month: 'Jul', sales: 295 },
            { month: 'Aug', sales: 288 },
            { month: 'Sep', sales: 279 },
            { month: 'Oct', sales: 285 },
            { month: 'Nov', sales: 270 }
        ];
        
        const radarMetrics = [
            { metric: 'Sales Efficiency', value: 95, benchmark: 70 },
            { metric: 'Customer Penetration', value: 98, benchmark: 65 },
            { metric: 'AOV Index', value: 85, benchmark: 60 },
            { metric: 'Space Efficiency', value: 92, benchmark: 70 },
            { metric: 'Market Position', value: 84, benchmark: 75 },
            { metric: 'Growth Trend', value: 55, benchmark: 60 }
        ];
        
        const competitiveData = [
            { name: 'Hy-Vee', score: 10, type: 'Complementary', color: '#22C55E' },
            { name: 'Cashwise', score: 9, type: 'Complementary', color: '#22C55E' },
            { name: 'Target', score: 8, type: 'Complementary', color: '#22C55E' },
            { name: 'Walmart', score: 7, type: 'Slight Comp', color: '#4A90D9' },
            { name: 'McDonald\'s', score: 4, type: 'Competitive', color: '#EF4444' },
            { name: 'Dunkin\'', score: 3, type: 'Competitive', color: '#EF4444' }
        ];
        
        // ========== TAB NAVIGATION ==========
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', () => {
                document.querySelectorAll('.nav-tab').forEach(t => t.classList.remove('active'));
                document.querySelectorAll('.tab-content').forEach(c => c.classList.remove('active'));
                tab.classList.add('active');
                document.getElementById(tab.dataset.tab).classList.add('active');
            });
        });
        
        // Sub-tabs
        document.querySelectorAll('.sub-tab').forEach(tab => {
            tab.addEventListener('click', () => {
                document.querySelectorAll('.sub-tab').forEach(t => t.classList.remove('active'));
                document.querySelectorAll('.sub-content').forEach(c => c.classList.remove('active'));
                tab.classList.add('active');
                document.getElementById(tab.dataset.subtab).classList.add('active');
            });
        });
        
        // ========== RENDER FUNCTIONS ==========
        function renderCategories() {
            const grid = document.getElementById('category-grid');
            grid.innerHTML = efficiencyScores.map(cat => `
                <div class="category-row">
                    <div class="status-dot ${cat.status}"></div>
                    <div class="category-name">${cat.category}</div>
                    <div class="progress-track">
                        <div class="progress-fill ${cat.status}" style="width: ${cat.score}%"></div>
                    </div>
                    <div class="category-score">${cat.score}</div>
                </div>
            `).join('');
        }
        
        function renderBubbles() {
            const container = document.getElementById('bubble-map');
            const maxSales = Math.max(...zipData.map(z => z.sales));
            
            zipData.forEach((zip, idx) => {
                const size = 45 + (zip.sales / maxSales) * 75;
                const bubble = document.createElement('div');
                bubble.className = 'zip-bubble' + (idx === 0 ? ' selected' : '');
                bubble.style.cssText = `
                    width: ${size}px; height: ${size}px;
                    left: calc(${zip.x}% - ${size/2}px);
                    top: calc(${zip.y}% - ${size/2}px);
                    background: radial-gradient(circle, ${zip.color}DD 0%, ${zip.color}50 70%, ${zip.color}15 100%);
                    border: 2px solid ${zip.color};
                `;
                bubble.innerHTML = `<span class="bubble-code">${zip.zip}</span><span class="bubble-pct">${zip.sales}%</span>`;
                bubble.onclick = () => selectZip(idx);
                container.appendChild(bubble);
            });
        }
        
        function renderZipList() {
            const list = document.getElementById('zip-list');
            list.innerHTML = zipData.map((zip, idx) => `
                <div style="display: flex; align-items: center; gap: 10px; padding: 8px 12px; border-radius: 6px; margin-bottom: 6px; cursor: pointer; background: ${idx === 0 ? 'rgba(74,144,217,0.1)' : 'transparent'};" onclick="selectZip(${idx})" data-zip-item="${idx}">
                    <div style="width: 12px; height: 12px; border-radius: 50%; background: ${zip.color};"></div>
                    <span style="flex: 1; font-size: 13px;">${zip.zip}</span>
                    <span style="font-size: 13px; font-weight: 600; color: var(--brand-primary);">${zip.sales}%</span>
                </div>
            `).join('');
        }
        
        function selectZip(idx) {
            const zip = zipData[idx];
            document.getElementById('selected-zip').innerHTML = `
                <div style="font-size: 36px; font-weight: 700; color: var(--brand-primary);">${zip.zip}</div>
                <div style="font-size: 20px; color: var(--brand-success); font-weight: 600;">${zip.sales}% of sales</div>
            `;
            document.querySelectorAll('.zip-bubble').forEach((b, i) => b.classList.toggle('selected', i === idx));
            document.querySelectorAll('[data-zip-item]').forEach((item, i) => {
                item.style.background = i === idx ? 'rgba(74,144,217,0.1)' : 'transparent';
            });
        }
        
        function renderAgeChart() {
            const container = document.getElementById('age-chart');
            container.innerHTML = ageData.map(d => `
                <div class="demo-group">
                    <div class="demo-label">${d.range}</div>
                    <div class="demo-bar-row">
                        <div class="demo-bar-track"><div class="demo-bar-fill shoppers" style="width: ${d.shoppers * 2.5}%">${d.shoppers}%</div></div>
                    </div>
                    <div class="demo-bar-row">
                        <div class="demo-bar-track"><div class="demo-bar-fill residents" style="width: ${d.residents * 2.5}%">${d.residents}%</div></div>
                    </div>
                </div>
            `).join('');
        }
        
        function renderIncomeChart() {
            const container = document.getElementById('income-chart');
            container.innerHTML = `
                <div style="display: flex; gap: 20px; margin-bottom: 16px; font-size: 12px;">
                    <span style="display: flex; align-items: center; gap: 6px;"><span style="width: 12px; height: 12px; background: var(--brand-primary); border-radius: 2px;"></span> Shoppers</span>
                    <span style="display: flex; align-items: center; gap: 6px;"><span style="width: 12px; height: 12px; background: var(--brand-accent); border-radius: 2px;"></span> Residents</span>
                </div>
            ` + incomeData.map(d => `
                <div class="demo-group">
                    <div class="demo-label">${d.range}</div>
                    <div class="demo-bar-row">
                        <div class="demo-bar-track"><div class="demo-bar-fill shoppers" style="width: ${d.shoppers * 2.5}%">${d.shoppers}%</div></div>
                    </div>
                    <div class="demo-bar-row">
                        <div class="demo-bar-track"><div class="demo-bar-fill residents" style="width: ${d.residents * 2.5}%">${d.residents}%</div></div>
                    </div>
                </div>
            `).join('');
        }
        
        function renderTrendChart() {
            const container = document.getElementById('trend-chart');
            const maxSales = Math.max(...monthlyTrend.map(m => m.sales));
            const minSales = Math.min(...monthlyTrend.map(m => m.sales));
            const range = maxSales - minSales + 40;
            
            container.innerHTML = monthlyTrend.map(m => {
                const height = ((m.sales - minSales + 20) / range) * 180;
                return `
                    <div class="chart-bar-wrapper">
                        <div class="chart-bar" style="height: ${height}px;"></div>
                        <div class="chart-bar-label">${m.month}</div>
                    </div>
                `;
            }).join('');
        }
        
        function renderRadarChart() {
            const svg = document.getElementById('radar-svg');
            const centerX = 0, centerY = 0, radius = 100;
            const n = radarMetrics.length;
            const angleStep = (2 * Math.PI) / n;
            
            // Grid circles
            [25, 50, 75, 100].forEach(r => {
                const circle = document.createElementNS('http://www.w3.org/2000/svg', 'circle');
                circle.setAttribute('cx', centerX);
                circle.setAttribute('cy', centerY);
                circle.setAttribute('r', r);
                circle.setAttribute('fill', 'none');
                circle.setAttribute('stroke', '#E2E8F0');
                circle.setAttribute('stroke-width', '1');
                svg.appendChild(circle);
            });
            
            // Grid lines and labels
            radarMetrics.forEach((m, i) => {
                const angle = i * angleStep - Math.PI / 2;
                const x = centerX + radius * Math.cos(angle);
                const y = centerY + radius * Math.sin(angle);
                
                const line = document.createElementNS('http://www.w3.org/2000/svg', 'line');
                line.setAttribute('x1', centerX);
                line.setAttribute('y1', centerY);
                line.setAttribute('x2', x);
                line.setAttribute('y2', y);
                line.setAttribute('stroke', '#E2E8F0');
                line.setAttribute('stroke-width', '1');
                svg.appendChild(line);
                
                const labelX = centerX + (radius + 25) * Math.cos(angle);
                const labelY = centerY + (radius + 25) * Math.sin(angle);
                const text = document.createElementNS('http://www.w3.org/2000/svg', 'text');
                text.setAttribute('x', labelX);
                text.setAttribute('y', labelY);
                text.setAttribute('text-anchor', 'middle');
                text.setAttribute('dominant-baseline', 'middle');
                text.setAttribute('font-size', '10');
                text.setAttribute('fill', '#64748B');
                text.textContent = m.metric.split(' ')[0];
                svg.appendChild(text);
            });
            
            // Benchmark polygon
            const benchmarkPoints = radarMetrics.map((m, i) => {
                const angle = i * angleStep - Math.PI / 2;
                const r = m.benchmark;
                return `${centerX + r * Math.cos(angle)},${centerY + r * Math.sin(angle)}`;
            }).join(' ');
            
            const benchmarkPoly = document.createElementNS('http://www.w3.org/2000/svg', 'polygon');
            benchmarkPoly.setAttribute('points', benchmarkPoints);
            benchmarkPoly.setAttribute('fill', 'rgba(100,116,139,0.2)');
            benchmarkPoly.setAttribute('stroke', '#64748B');
            benchmarkPoly.setAttribute('stroke-width', '2');
            benchmarkPoly.setAttribute('stroke-dasharray', '5,5');
            svg.appendChild(benchmarkPoly);
            
            // Value polygon
            const valuePoints = radarMetrics.map((m, i) => {
                const angle = i * angleStep - Math.PI / 2;
                const r = m.value;
                return `${centerX + r * Math.cos(angle)},${centerY + r * Math.sin(angle)}`;
            }).join(' ');
            
            const valuePoly = document.createElementNS('http://www.w3.org/2000/svg', 'polygon');
            valuePoly.setAttribute('points', valuePoints);
            valuePoly.setAttribute('fill', 'rgba(30,58,95,0.3)');
            valuePoly.setAttribute('stroke', '#1E3A5F');
            valuePoly.setAttribute('stroke-width', '2');
            svg.appendChild(valuePoly);
            
            // Value points
            radarMetrics.forEach((m, i) => {
                const angle = i * angleStep - Math.PI / 2;
                const r = m.value;
                const x = centerX + r * Math.cos(angle);
                const y = centerY + r * Math.sin(angle);
                
                const circle = document.createElementNS('http://www.w3.org/2000/svg', 'circle');
                circle.setAttribute('cx', x);
                circle.setAttribute('cy', y);
                circle.setAttribute('r', '5');
                circle.setAttribute('fill', '#1E3A5F');
                svg.appendChild(circle);
            });
        }
        
        function renderCompMatrix() {
            const container = document.getElementById('comp-matrix');
            container.innerHTML = competitiveData.map(c => `
                <div class="comp-row">
                    <div class="comp-score" style="background: ${c.color};">${c.score}</div>
                    <div class="comp-info">
                        <div class="comp-name">${c.name}</div>
                    </div>
                    <span class="comp-type" style="background: ${c.color}20; color: ${c.color};">${c.type}</span>
                </div>
            `).join('');
        }
        
        // ========== INITIALIZE ==========
        document.addEventListener('DOMContentLoaded', () => {
            renderCategories();
            renderBubbles();
            renderZipList();
            renderAgeChart();
            renderIncomeChart();
            renderTrendChart();
            renderRadarChart();
            renderCompMatrix();
        });
    </script>
</body>
</html>
